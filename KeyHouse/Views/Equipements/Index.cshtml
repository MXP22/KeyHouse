@model IEnumerable<KeyHouse.Models.Equipement>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NomEquipement)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Logements.TitreLogemenet)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NomEquipement)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Logements.TitreLogemenet)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IDequipement }) |
            @Html.ActionLink("Details", "Details", new { id=item.IDequipement }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IDequipement })
        </td>
    </tr>
}

</table>
